  "license": "MIT",
  "author": {
    "name": "Raphael Schnaitl",
    "email": "raphael@ventx.de",
    "url": "https://ventx.de"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ventx/aws-cdk-library.git"

  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak -v"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "dotnet": {
        "namespace": "Ventx.AwsCdkLibrary",
        "packageId": "Ventx.AwsCdkLibrary"
      },
      "python": {
        "distName": "ventx.aws-cdk-library",
        "module": "ventx.aws_cdk_library"
      }
    }
  },
  "devDependencies": {
    "@types/nodeunit": "^0.0.30",
    "jsii": "^0.11.3",
    "jsii-pacmak": "^0.11.3",
    "nodeunit": "^0.11.2",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "@aws-cdk/cdk": "^0.34.0",
    "@aws-cdk/aws-sns": "^0.34.0",
    "@aws-cdk/aws-sqs": "^0.34.0"
  },
  "dependencies": {
    "@aws-cdk/aws-sns": "^0.34.0",
    "@aws-cdk/aws-sqs": "^0.34.0",
    "@aws-cdk/cdk": "^0.34.0"
  }
}
